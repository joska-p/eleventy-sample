{
  "name": "eleventy-sample",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "npx @11ty/eleventy && tailwindcss -i src/css/styles.css -o public/css/styles.css",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "tailwindcss -i src/css/styles.css -o public/css/styles.css --watch --postcss",
    "build-ghpages": "gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joska-p/eleventy-sample.git"
  },
  "homepage": "https://joska-p.github.io/eleventy-sample",
  "bugs": {
    "url": "https://github.com/joska-p/eleventy-sample/issues"
  },
  "author": "joska-p",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "dayjs": "^1.11.13",
    "netlify-plugin-cache": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "gh-pages": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.14"
  }
}
